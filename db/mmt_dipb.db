# BOOL
# READ ONLY

record(bi, "$(P)MMT_DIPB1_FM$(ID)_FlowOK")
{
	field(SCAN, ".5 second")
	field(DTYP, "EtherIP")
	field( INP, "@$(PLC) MMT_DIPB1_FM$(ID)_FlowOK")
	field(ZNAM, "False")
	field(ONAM, "True")
	field( ZSV, "NO_ALARM")
	field( OSV, "MAJOR")
}

